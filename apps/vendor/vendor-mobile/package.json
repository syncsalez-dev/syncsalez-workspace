{
  "name": "@syncsalez-dev/vendor-mobile",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "name": "vendor-mobile",
    "projectType": "application",
    "sourceRoot": "apps/vendor/vendor-mobile/src",
    "targets": {
      "start": {
        "executor": "@nx/expo:start",
        "dependsOn": [
          "sync-deps"
        ],
        "options": {}
      },
      "serve": {
        "executor": "@nx/expo:serve",
        "dependsOn": [
          "sync-deps"
        ],
        "options": {
          "port": 4200
        }
      },
      "run-ios": {
        "executor": "@nx/expo:run",
        "dependsOn": [
          "sync-deps"
        ],
        "options": {
          "platform": "ios"
        }
      },
      "run-android": {
        "executor": "@nx/expo:run",
        "dependsOn": [
          "sync-deps"
        ],
        "options": {
          "platform": "android"
        }
      },
      "build": {
        "executor": "@nx/expo:build",
        "dependsOn": [
          "sync-deps"
        ],
        "options": {}
      },
      "submit": {
        "executor": "@nx/expo:submit",
        "options": {}
      },
      "build-list": {
        "executor": "@nx/expo:build-list",
        "options": {}
      },
      "sync-deps": {
        "executor": "@nx/expo:sync-deps",
        "options": {}
      },
      "prebuild": {
        "executor": "@nx/expo:prebuild",
        "dependsOn": [
          "sync-deps"
        ],
        "options": {}
      },
      "install": {
        "executor": "@nx/expo:install",
        "options": {}
      },
      "update": {
        "executor": "@nx/expo:update",
        "options": {}
      },
      "export": {
        "executor": "@nx/expo:export",
        "dependsOn": [
          "sync-deps"
        ],
        "outputs": [
          "{options.outputDir}"
        ],
        "options": {
          "platform": "all",
          "outputDir": "dist/apps/vendor/vendor-mobile"
        }
      },
      "serve-static": {
        "executor": "@nx/web:file-server",
        "dependsOn": [
          "export"
        ],
        "options": {
          "buildTarget": "vendor-mobile:export",
          "staticFilePath": "dist/apps/vendor/vendor-mobile",
          "spa": true
        }
      }
    }
  },
  "scripts": {
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "dependencies": {
    "expo": "*",
    "formdata-polyfill": "*",
    "@nx/devkit": "*",
    "@nx/eslint": "*",
    "@nx/jest": "*",
    "@nx/js": "*",
    "@nx/react": "*",
    "@nx/web": "*",
    "@nx/webpack": "*",
    "enhanced-resolve": "*",
    "metro-config": "*",
    "metro-resolver": "*",
    "node-fetch": "*",
    "picocolors": "*",
    "tsconfig-paths": "*",
    "tslib": "*",
    "@expo/metro-config": "*",
    "react-native-svg-transformer": "*",
    "react": "*",
    "@testing-library/react-native": "*",
    "react-native": "*",
    "react-native-svg": "*"
  },
  "devDependencies": {}
}